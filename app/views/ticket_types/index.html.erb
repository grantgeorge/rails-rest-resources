<p id="notice"><%= notice %></p>

<h1>Listing Ticket Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Quantity</th>
      <th>Price</th>
      <th>Number of registrations</th>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_types.each do |ticket_type| %>
      <tr>
        <td><%= ticket_type.name %></td>
        <td><%= ticket_type.quantity %></td>
        <td><%= ticket_type.price %></td>
        <td><%= ticket_type.number_of_registrations %></td>
        <td><%= ticket_type.event %></td>
        <td><%= link_to 'Show', ticket_type %></td>
        <td><%= link_to 'Edit', edit_ticket_type_path(ticket_type) %></td>
        <td><%= link_to 'Destroy', ticket_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket type', new_ticket_type_path %>
